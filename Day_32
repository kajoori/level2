    <h1>coalescing</h1>

    <script>
        document.write("<br>");
        //coalescing
        //x ?? y
        let x=10;
        let y;
        y=y ?? 2;
      // document.write(x ?? 5);
       document.write(x/y);
    </script>
    <br>
    <hr>
     <h1>If else</h1>
     <script>
     let kaju=true;
        if(!kaju){
            document.write("kayu ready to give presentation by tomorrow")
        } 
        else{
            document.write("she is not")
        }
     </script>
     <br>
    
    <script> 
        document.write("<br>");
        let kayu=20;
        if (kayu>70 ) {
        document.write("kayu is above 70");}
        else if(kayu<70 && kayu > 50){
            document.write(" kayu in second stage")
        }
       else {
        document.write("kayu in third stage")
       }
       document.write("<br>");
       document.write("after if condition")
    </script>
<br>
