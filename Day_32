<h1>Forloop</h1>
<h3>1</h3>
<script>
    let flowers =["Rose","Lotus","Sunflower"];
    for (let n = 0; n < flowers.length; n++) {
         document.write(flowers[n] + "<br>");
 }
          
</script>
<h3>2</h3>
<script>
   let colours =["Black","Pink","Green"];
   for (let m = 0; m < colours.length; m++){
       document.write(colours[m] + "<br>")
   }
</script>
   
